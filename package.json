{
  "name": "generate-i18",
  "displayName": "USUM Generate I18",
  "description": "Support actions related to i18",
  "publisher": "USUM-Software",
  "author": "UsumSoftwareTeam, usumsupport <support@usumsoftware.com>",
  "copyright": "UsumSoftwareTeam",
  "homepage": "",
  "license": "MIT",
  "bugs": {
    "url": "https://usumsoftware.com",
    "email": "support@usumsoftware.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phatphamm01/USUM-Generate-I18"
  },
  "icon": "media/usum-logo.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.65.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "properties": {
      "usum-generate-i18.linkFolder": {
        "default": "usum",
        "description": "Link folder local",
        "type": "string"
      },
      "usum-generate-i18.language": {
        "default": "en",
        "description": "Language ?",
        "type": "string"
      }
    },
    "commands": [
      {
        "command": "generate-i18.generateTranslateJson",
        "title": "Generate translation file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0",
    "lodash": "^4.17.21"
  }
}
